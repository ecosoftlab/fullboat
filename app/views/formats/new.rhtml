<h1>Create a New Format</h1>

<% remote_form_for(:format, :url => formats_path) do |f| %>
	<fieldset id="format_details" name="Format Details" class="clearfix">
		<%= render(:partial => "formats/form", :locals => {:f => f})%>
    	<%= submit_tag "Create" %>
	</fieldset>
<% end %>

<%= link_to 'Back', formats_path %>