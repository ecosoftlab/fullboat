<h1>Editing Psa '<%= @psa%>'</h1>

<% remote_form_for(:psa, :url => psa_path(@psa), :html => { :method => :put }) do |f| %>
	<fieldset id="psa_details" name="Psa Details" class="clearfix">
		<%= render(:partial => "psa/form", :locals => {:f => f})%>
    	<%= submit_tag "Update" %>
	</fieldset>
<% end %>

<%= link_to 'Show', psa_path(@psa) %> |
<%= link_to 'Back', psas_path %>