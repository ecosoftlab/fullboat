<h1>Create a New Review</h1>

<% remote_form_for(:review, :url => reviews_path) do |f| %>
	<fieldset id="review_details" name="Review Details" class="clearfix">
		<%= render(:partial => "reviews/form", :locals => {:f => f})%>
    	<%= submit_tag "Create" %>
	</fieldset>
<% end %>

<%= link_to 'Back', reviews_path %>